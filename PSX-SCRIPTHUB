GUI local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("SLIT HUB (Made by Slit#7960", "DarkTheme")

-- DUPE
local Dupe = Window:NewTab("DUPE")
local DupeSection = Dupe:NewSection("Dupe v2 (OP)")


DupeSection:NewButton("DUPE (wait 30 secs after clicking button)", "OP DUPE SCRIPT", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Project-WD/SCRIPTHUB/main/Main.Lua"))()
end)

TRADESCAMSection:NewButton("ONLY FOR TRADING )", "OP TRADESCAM SCRIPT", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Project-WD/SCRIPTHUB/main/Main.Lua"))()
end)



